    "type": "git",
    "url": "git+https://github.com/dmurcia/twitter-posts.git"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "typescript": "4.1.6",
    "@changesets/cli": "^2.22.0"
  }
}
